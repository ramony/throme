{
  "name": "Throme",
  "manifest_version": 2,
  "version": "1.0.0",
  "app": {
    "launch": {
      "local_path": "index.html"
    }
  },
  "background": {
    "scripts": []
  },
  "description": "__MSG_description__",
  "permissions": [
    "notifications",
    "tabs",
    "http://*/*",
    "https://*/*",
    "contextMenus",
    "unlimitedStorage",
    "webRequest",
    "webRequestBlocking"
  ]
}